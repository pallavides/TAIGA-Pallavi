#include "utilities/axi_spi.h"

void supervisor_send_state_vector(float stateVector[4]){
	u8 uartBuffer[16];

	u32_to_buffer((u32)stateVector[0], &uartBuffer[0], 4);
	u32_to_buffer((u32)stateVector[1], &uartBuffer[4], 4);
	u32_to_buffer((u32)stateVector[2], &uartBuffer[8], 4);
	u32_to_buffer((u32)stateVector[3], &uartBuffer[12], 4);


}

void supervisor_send_tail(){

}
